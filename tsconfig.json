{
  "compilerOptions": {
    "sourceMap": true,
    "noImplicitAny": false,
    "target": "ES5",                                  /* Set the JavaScript language version for emitted JavaScript and include compatible library declarations. */
    "module": "ES2020",
    "outDir": "./dist/",
    "rootDir": "./src/",
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "downlevelIteration": true
  },
  "exclude": [
    "node_modules"
  ]
}
